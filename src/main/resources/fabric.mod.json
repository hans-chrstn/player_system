{
	"schemaVersion": 1,
	"id": "player_system",
	"version": "${version}",
	"name": "player_system",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/player_system/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "mishima.player_system.Player_system",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "mishima.player_system.Player_systemClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "mishima.player_system.Player_systemDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"player_system.mixins.json",
		{
			"config": "player_system.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.5",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}